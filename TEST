package com.company;

public class Main {

    public static void main(String[] args) {

        {
            int age = 30;

            switch (age) {
                case 10:
                    System.out.print("way too young!");
                    break;
                case 20:
                    System.out.print("just right");
                    break;
                case 30:
                    System.out.print("way too old!");
                    break;
                default:
                    System.out.print("not allow");
            }
        }
    }
}
